{
  "name": "affiliate-platform",
  "version": "1.0.0",
  "description": "Lightning-fast affiliate referral platform with Telegram bot",
  "main": "server.js",
  "scripts": {
    "start": "set LOG_LEVEL=info && set PERFORMANCE_MODE=false && node --max-old-space-size=1024 server.js",
    "dev": "set LOG_LEVEL=error && set PERFORMANCE_MODE=true && nodemon --max-old-space-size=2048 server.js",
    "dev:bot": "set LOG_LEVEL=error && set PERFORMANCE_MODE=true && nodemon --exec node --max-old-space-size=1024 bot/index.js",
    "test": "jest",
    "lint": "echo 'No linting configured'",
    "keep-alive": "node scripts/keep-alive.js",
    "setup": "node scripts/setup.js",
    "optimize": "node scripts/optimize-logging.js"
  },
  "keywords": [
    "affiliate",
    "referral",
    "telegram",
    "nextjs",
    "firebase"
  ],
  "author": "Cody AI",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.17.1",
    "redis": "^4.7.1",
    "stripe": "^18.3.0",
    "telegraf": "^4.15.6",
    "telegraf-session-local": "^2.1.1",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3"
  }
}
